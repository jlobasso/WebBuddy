{
  "name": "__MSG_appName_",
  "version": "1.0",
  "description": "__MSG_appDescription__",
  "manifest_version": 2,
  "default_locale": "en",
  "background": {
    "scripts": [
      "./background/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./content/content.js"
      ],
      "all_frames": true
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "https://*/",
    "http://*/"
  ],
  "page_action": {
    "default_popup": "popupWindows/popup.html"
  },
  "web_accessible_resources": [
    "content/pulpoNav.html"
  ]
}