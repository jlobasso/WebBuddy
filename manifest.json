{
  "name": "__MSG_pulpouName__",
  "version": "1.1",
  "description": "__MSG_pulpouDescription__",
  "manifest_version": 2,
  "default_locale": "en",
  "background": {
    "scripts": [
      "background/Profile.js",
      "background/Background-login.js",
      "background/main-background.js",
      "background/background-web-buddy-bar.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "https://*/",
    "http://*/",
    "contextMenus"
  ],
  "browser_action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "128": "popup/images/get_pulpou128_disabled.png"
    }
  },
  "icons": {
    "16": "popup/images/get_pulpou16.png",
    "32": "popup/images/get_pulpou32.png",
    "48": "popup/images/get_pulpou48.png",
    "128": "popup/images/get_pulpou128.png"
  },
  "web_accessible_resources": [
    "content/web-buddy-bar/pulpoNav.html"
  ]
}