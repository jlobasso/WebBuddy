{
  "name": "__MSG_appName__",
  "version": "1.0",
  "description": "__MSG_appDescription__",
  "manifest_version": 2,
  "default_locale": "en",
  "background": {
    "scripts": [
      "./background/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./content/content.js"
      ],
      "all_frames": true
    }
  ],
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "https://*/",
    "http://*/",
    "declarativeContent"
  ],
  "page_action": {
    "default_popup": "popupWindows/popup.html",
    "default_icon": {
      "16": "popupWindows/images/get_pulpou16.png",
      "32": "popupWindows/images/get_pulpou32.png",
      "48": "popupWindows/images/get_pulpou48.png",
      "128": "popupWindows/images/get_pulpou128.png"
    }
  },
  "icons": {
    "16": "popupWindows/images/get_pulpou16.png",
    "32": "popupWindows/images/get_pulpou32.png",
    "48": "popupWindows/images/get_pulpou48.png",
    "128": "popupWindows/images/get_pulpou128.png"
  },
  "web_accessible_resources": [
    "content/pulpoNav.html",
    "content/interface.js"
  ]
}